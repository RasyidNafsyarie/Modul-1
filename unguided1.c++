#include <iostream>
using namespace std;

void fungsiTambah() {
    int x = 10;
    int y = 20;
    cout << "Hasil Penjumlahan : " << x + y << endl;
}
void fungsiBagi() {
    float x = 10;
    float y = 3.5;
    cout << "Hasil Pembagian : " << x / y << endl;
}
int main() {
    fungsiTambah();
    fungsiBagi();
    return 0;
}

